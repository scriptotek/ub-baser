{
    "name": "scriptotek/ub-baser",
    "description": "UB-baser",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "aacotroneo/laravel-saml2": "dev-patch-1",
        "barryvdh/laravel-debugbar": "~3.0",
        "doctrine/dbal": "~2.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "~5.8",
        "punic/punic": "^3.3",
        "sentry/sentry-laravel": "^1.2",
        "stevenmaguire/laravel-middleware-csp": "^0.1.1"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "nunomaduro/collision": "^3.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/danmichaelo/laravel-saml2"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
